<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="customer" class="com.mkyong.common.Customer" autowire="byName" />
		
	<bean name="address1" id="address1" class="com.mkyong.common.Address" >
		<property name="fulladdress" value="address1" />
	</bean>
	<bean name="address" id="address" class="com.mkyong.common.Address" >
		<property name="fulladdress" value="Block A 888, CA1" />
	</bean>
</beans>